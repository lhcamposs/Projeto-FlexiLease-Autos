    "/reserve": {
      "post": {
        "summary": "Registra uma reserva",
        "tags": [
          "Reservas"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "startDate",
                  "endDate",
                  "carId"
                ],
                "properties": {
                  "startDate": {
                    "type": "string",
                    "example": "20/04/2023",
                    "description": "Data de início da reserva"
                  },
                  "endDate": {
                    "type": "string",
                    "example": "30/04/2023",
                    "description": "Data de término da reserva"
                  },
                  "carId": {
                    "type": "integer",
                    "example": 1,
                    "description": "ID do carro a ser reservado"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Reserva criada com sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "integer",
                      "example": 1
                    },
                    "startDate": {
                      "type": "string",
                      "example": "20/04/2023"
                    },
                    "endDate": {
                      "type": "string",
                      "example": "30/04/2023"
                    },
                    "finalValue": {
                      "type": "string",
                      "example": "500,00"
                    },
                    "userId": {
                      "type": "integer",
                      "example": 1
                    },
                    "carId": {
                      "type": "integer",
                      "example": 1
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Erro de validação dos dados fornecidos"
          }
        }
