    "/user": {
      "post": {
        "summary": "Registra um usuário",
        "tags": [
          "Usuários"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "name",
                  "cpf",
                  "birth",
                  "cep",
                  "email",
                  "password"
                ],
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "Joãozinho Ciclano"
                  },
                  "cpf": {
                    "type": "string",
                    "example": "131.147.860-49",
                    "description": "CPF válido e único no sistema"
                  },
                  "birth": {
                    "type": "string",
                    "format": "date",
                    "example": "03/03/2000"
                  },
                  "cep": {
                    "type": "string",
                    "example": "90010-510",
                    "description": "O CEP será usado para buscar o endereço completo através da API Via CEP"
                  },
                  "email": {
                    "type": "string",
                    "example": "joazinho@email.com",
                    "description": "Email válido e único no sistema"
                  },
                  "password": {
                    "type": "string",
                    "format": "password",
                    "example": "123456",
                    "description": "A senha deve ter pelo menos 6 caracteres"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Usuário registrado com sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "integer",
                      "example": 1
                    },
                    "name": {
                      "type": "string",
                      "example": "Joãozinho Ciclano"
                    },
                    "cpf": {
                      "type": "string",
                      "example": "131.147.860-49"
                    },
                    "birth": {
                      "type": "string",
                      "format": "date",
                      "example": "03/03/2000"
                    },
                    "email": {
                      "type": "string",
                      "example": "joazinho@email.com"
                    },
                    "qualified": {
                      "type": "boolean",
                      "example": true,
                      "description": "Indica se o usuário é maior de idade (18+)"
                    },
                    "cep": {
                      "type": "string",
                      "example": "90010-510"
                    },
                    "neighbordhood": {
                      "type": "string",
                      "example": "Centro Histórico"
                    },
                    "street": {
                      "type": "string",
                      "example": "Largo do Trabalho"
                    },
                    "complement": {
                      "type": "string",
                      "example": ""
                    },
                    "city": {
                      "type": "string",
                      "example": "Porto Alegre"
                    },
                    "uf": {
                      "type": "string",
                      "example": "RS"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Erro de validação dos dados fornecidos"
          }
        }
      }
