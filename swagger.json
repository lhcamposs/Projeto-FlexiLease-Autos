                    "total": {
                      "type": "integer",
                      "example": 100
                    },
                    "limit": {
                      "type": "integer",
                      "example": 10
                    },
                    "offset": {
                      "type": "integer",
                      "example": 1
                    },
                    "offsets": {
                      "type": "integer",
                      "example": 10
                    }
                  }
                }
              }
